{
    "manifest_version": 3,
    "name": "Bg-Changer",
    "icons": {
        "32": "icon322.png" 
    },
    "version": "1.0.0",
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "action": { 
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting", "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}